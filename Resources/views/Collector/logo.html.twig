{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set icon %}
    <a href="http://symfony.com/">
        <img height="28" width="114" alt="Smile" src="data:image/png;base64," />
    </a>
    {% endset %}
    {% set text %}
    <div class="sf-toolbar-info-piece" style="text-align:center">
        {% if collector.trainingMode %}
            <img height="35" width="33" alt="Banana \o/" src="data:image/gif;base64," />
            <br />
            Smile training mode is <strong style="color:green">ON</strong>
        {% else %}
            Smile training mode is <strong style="color:red">OFF</strong> :(
        {% endif %}
    </div>
    {% endset %}
    {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': false } %}
{% endblock %}